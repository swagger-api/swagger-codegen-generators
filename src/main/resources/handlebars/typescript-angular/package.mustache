{
  "name": "{{{npmName}}}",
  "version": "{{{npmVersion}}}",
  "description": "swagger client for {{{npmName}}}",
  "author": "Swagger Codegen Contributors",
  "keywords": [
    "swagger-client"
  ],
  "license": "Unlicense",
  {{#useNgPackagr}}
  "scripts": {
    "build": "ng-packagr -p ng-package.json"
  },
  {{/useNgPackagr}}
  {{^useNgPackagr}}
  "main": "dist/index.js",
  "module": "dist/index.js",  
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "ngc && npm pack"
  },
  {{/useNgPackagr}}
  "peerDependencies": {
    "@angular/core": "^{{ngVersion}}",{{#useHttpClientPackage}}
    "@angular/http": "^{{ngVersion}}",{{/useHttpClientPackage}}
    "@angular/common": "^{{ngVersion}}",
    "@angular/compiler": "^{{ngVersion}}",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^{{rxjsVersion}}"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^{{ngVersion}}",
    "@angular/core": "^{{ngVersion}}",{{#useHttpClientPackage}}
    "@angular/http": "^{{ngVersion}}",{{/useHttpClientPackage}}
    "@angular/common": "^{{ngVersion}}",
    "@angular/compiler": "^{{ngVersion}}",
    "@angular/platform-browser": "^{{ngVersion}}",{{#useNgPackagr}}
    "ng-packagr": "^{{ngPackagrVersion}}",{{/useNgPackagr}}
    "reflect-metadata": "^0.1.3",
    "rxjs": "^{{rxjsVersion}}",
    "typescript": "{{{tsVersion}}}",
    "zone.js": "^{{zonejsVersion}}"
  }{{#npmRepository}},{{/npmRepository}}
{{#npmRepository}}
  "publishConfig": {
    "registry": "{{{npmRepository}}}"
  }
{{/npmRepository}}
}
